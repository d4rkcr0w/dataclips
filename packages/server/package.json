    "@nestjs/platform-express": "^8.4.0",
    "@nestjs/serve-static": "^2.2.2",
    "@types/bytes": "^3.1.1",
    "apollo-server-core": "^3.6.2",
    "apollo-server-errors": "^3.3.1",
    "apollo-server-express": "^3.6.2",
    "argon2": "^0.28.5",
