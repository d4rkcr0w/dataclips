{
  "private": true,
  "name": "@dataclips/server",
  "version": "0.0.0-development",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn clean",
    "build": "nest build",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:server": "node dist/main",
    "start:worker": "node dist/worker",
    "dev": "nest start --watch",
    "debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "nest-boot": "nest-boot"
  },
  "dependencies": {
    "@apollo/gateway": "^0.46.0",
    "@nest-boot/command": "^1.3.7",
    "@nest-boot/common": "^1.2.7",
    "@nest-boot/database": "^1.3.8",
    "@nest-boot/graphql": "^1.3.11",
    "@nest-boot/logger": "^1.2.3",
    "@nest-boot/queue": "^1.2.7",
    "@nest-boot/redis": "^1.3.4",
    "@nest-boot/search": "^1.3.8",
    "@nest-boot/search-engine-meilisearch": "^1.1.8",
    "@nestjs/apollo": "^10.0.4",
    "@nestjs/common": "^8.3.1",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.3.1",
    "@nestjs/graphql": "^10.0.4",
    "@nestjs/platform-express": "^8.3.1",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/typeorm": "^8.0.3",
    "@types/bytes": "^3.1.1",
    "apollo-server-core": "^3.6.2",
    "apollo-server-errors": "^3.3.1",
    "apollo-server-express": "^3.6.2",
    "argon2": "^0.28.4",
    "aws-sdk": "^2.1078.0",
    "bullmq": "^1.74.1",
    "bytes": "^3.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "dataloader": "^2.0.0",
    "express": "^4.17.3",
    "fs-extra": "^10.0.0",
    "graphql": "^15.8.0",
    "graphql-type-json": "^0.3.2",
    "ioredis": "^4.28.4",
    "meilisearch": "^0.24.0",
    "ms": "^2.1.3",
    "mysql2": "^2.3.3",
    "nestjs-pino": "^2.5.0",
    "node-xlsx": "^0.21.0",
    "papaparse": "^5.3.1",
    "pg": "^8.7.3",
    "pino": "^6.13.4",
    "pino-http": "^6.6.0",
    "redis-info": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "source-map-support": "^0.5.21",
    "ts-morph": "^13.0.3",
    "typeorm": "^0.2.43"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.3.1",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.7",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.19",
    "@types/papaparse": "^5.3.2",
    "@types/pg": "^8.6.4",
    "@types/source-map-support": "^0.5.4",
    "@types/supertest": "^2.0.11",
    "jest": "^27.4.7",
    "nanoid": "^3.3.1",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
