    "@mikro-orm/reflection": "^5.1.2",
    "@nest-boot/command": "^2.0.0",
    "@nest-boot/common": "^2.0.0",
    "@nest-boot/database": "^2.0.0",
    "@nest-boot/graphql": "^2.0.0",
    "@nest-boot/queue": "^2.0.0",
